class Solution {
public:
    int maxDepth(TreeNode* root) {
        int ans=0;
        if(!root)   return ans;
        else
        {
            int lheight = maxDepth(root->left)   ;
            int rheight = maxDepth(root->right);
            if(lheight>rheight) ans = lheight+1;
            else    ans = rheight+1;
        }
        return ans;
    }
};
