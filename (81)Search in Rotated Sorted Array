class Solution {
public:
    bool search(vector<int>& nums, int target) 
    {
        int n = nums.size();
        bool flag = false;
        
        for(int i=0 ; i<n ; i++)
        {
            if(target==nums[i])
            {
               flag=true;         
                break;
            }
        }
    return flag;    
    }
};
